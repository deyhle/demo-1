<!DOCTYPE html>
<html dir="ltr" lang="de-DE">
<head>
  <meta http-equiv="content-type" content="text/html; charset=utf-8"/>
  <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1"/>

  <!-- Stylesheets
  ============================================= -->
    {{#each _embedded.b95d1dd8:stylesheet}}
      <link rel="stylesheet" type="text/css" href="{{href}}"/>
    {{/each}}

  <!--[if lt IE 9]>
    <script src="https://css3-mediaqueries-js.googlecode.com/svn/trunk/css3-mediaqueries.js"></script>
  <![endif]-->

  <!-- Externes Javascript
  ============================================= -->
    {{#each _embedded.b95d1dd8:script}}
        {{#unless footer}}
          <script type="text/javascript"{{#if src}} src="{{src}}"{{/if}}>{{{inlineScript}}}</script>{{/unless}}
    {{/each}}

  <!-- Titel der Webseite
  ============================================= -->
  <title>{{websiteTitle}}</title>

</head>

<body class="stretched side-push-panel">

<div class="body-overlay"></div>

<!-- Header
    ============================================= -->
<header id="header" class="full-header transparent-header dark static-sticky" data-sticky-class="not-dark"
        data-sticky-offset="full" data-sticky-offset-negative="100">

  <div id="header-wrap">

    <div class="container clearfix">

      <div id="primary-menu-trigger"><i class="icon-reorder"></i></div>

      <!--

      Logo
      ========================…

      -->

      <div id="logo">
        <a class="standard-logo" href="index.html">
          <img alt="Canvas Logo" src="{{logo}}"></img>
        </a>
      </div>
      <!--

       #logo end

      -->

      <!-- Hauptnavigationsleiste Start
      ============================================= -->
      <nav id="primary-menu">

        <ul class="one-page-menu" data-easing="easeInOutExpo" data-speed="1250" data-offset="65">
            {{#each partials}}
              <li><a href="#" data-href="#anchor_{{name}}">
                <div style="text-transform: uppercase;">{{navTitle}}</div>
              </a></li>
            {{/each}}
        </ul>

        <!-- Until appsite supports it, disabled 	<div id="side-panel-trigger"><a href="#"><i class="icon-reorder"></i></a></div> -->

      </nav>
      <!-- Hauptnavigationsleiste Ende -->

    </div>

  </div>

</header>
<!-- #header end -->


<!-- Document Wrapper
============================================= -->
<div id="wrapper" class="clearfix">
  <!-- iterate through partials here -->

    {{#each partials}}
      <div id="anchor_{{name}}">
          {{> (lookup . 'name') this }}
      </div>
    {{/each}}

  <div class="content-wrap nopadding"> <!-- start nopadding -->


  </div>
  <!-- #wrapper end -->

  <!-- Go To Top
  ============================================= -->
  <div id="gotoTop" class="icon-angle-up"></div>

  <!-- Footer Scripts
  ============================================= -->
    {{#each _embedded.b95d1dd8:script}}
        {{#if footer}}
          <script type="text/javascript"{{#if src}} src="{{src}}"{{/if}}>{{{inlineScript}}}</script>{{/if}}
    {{/each}}

</div>
<!-- Footer
============================================= -->
